@page "/createTransaction/{ArticleId}}"

<TransactionEdit StartType="@(Enum.Parse<TransactionType>(TypeString))" ArticleId="@ArticleId"/>

@code {
    [Parameter]
    public string ArticleId { get; set; }

    [Parameter]
    [SupplyParameterFromQuery(Name = "Type")]
    public string TypeString { get; set; }
}